{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "ListExistingRolesAndPolicies",
			"Effect": "Allow",
			"Action": [
				"iam:ListRolePolicies",
				"iam:ListRoles"
			],
			"Resource": "*"
		},
		{
			"Sid": "PermissionToPassAnyRole",
			"Effect": "Allow",
			"Action": [
				"iam:PassRole"
			],
			"Resource": "arn:aws:iam::721266161153:role/*"
		}
	]
}


